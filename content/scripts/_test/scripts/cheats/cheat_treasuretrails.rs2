if (p_finduid(uid) = true) {
    def_int $choice = ~p_choice4_header("Easy", 0, "Medium", 1, "Hard", 2, "Cancel", 3, "Select Clue Tier");
    switch_int ($choice) {
        case 0 : @debug_clues_easy;
        case 1 : @debug_clues_medium;
        case 2 : @debug_clues_hard;
        case 3 : return;
    }
}

