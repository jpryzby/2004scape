[debugproc,hunt_hobgoblin]
while(p_finduid(uid) = true){
    ~walk_to_hobgoblins;
    ~hunt_hobgoblins;
    ~return_from_hobgoblins;

    ~bot_bank_deposit_all(0_47_52_4_26,0);
    ~bot_heal(0_47_52_4_26);
}





