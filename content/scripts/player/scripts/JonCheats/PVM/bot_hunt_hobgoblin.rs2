[proc,hunt_hobgoblins]
while(inv_freespace(inv) > 0 & stat(hitpoints) > 15 & p_finduid(uid) = true){
    def_coord $loot_coord = ~fight(hobgoblin_unarmed);
    ~loot_hobgoblin($loot_coord);
    equip_arrows;
}



[proc,loot_hobgoblin](coord $hobgobCoord)
obj_findallzone($hobgobCoord);
while(obj_findnext() = true & inv_freespace(inv) > 0){//////////////////////////////this can be improved
    ~botwalk($hobgobCoord);
    switch_obj(active_obj) {
        case bones : ~bury;

        case bronze_spear : ~destroy;
        case iron_spear : ~destroy;
        case steel_spear : ~destroy;
        case iron_sword : ~destroy;
        case steel_dagger : ~destroy;
        case steel_longsword : ~destroy;
        case unidentified_guam : ~destroy;
        case unidentified_marrentill : ~destroy;
        case unidentified_tarromin : ~destroy;
        case unidentified_harralander : ~destroy;
        case waterrune : ~destroy;
        case firerune : ~destroy;
        case bodyrune : ~destroy;
        case chaosrune : ~destroy;
        case naturerune : ~destroy;
        case lawrune : ~destroy;
        case cosmicrune : ~destroy;
        case coins : ~destroy;
        case snape_grass : ~destroy;
        case goblin_armour : ~destroy;
        case uncut_sapphire : ~destroy;
        case uncut_emerald : ~destroy;

        case bronze_arrow : ~pick_up(0);
        case bronze_knife : ~pick_up(0);
        case iron_knife : ~pick_up(0);

        case default : ~pick_up(1);
    }
    obj_findallzone($hobgobCoord); 
}